<?xml version="1.0"?>
<mvc>
  <profile name="openurp-edu-stat-webapp" package="org.openurp.edu.stat.web.action">
    <url path="/" suffix="" style="seo"/>
    <interceptors>
      <interceptor name="web.Interceptor.hibernate"/>
      <interceptor name="web.Interceptor.security"/>
    </interceptors>
  </profile>
  <static>
    <bundle name="echarts" version="4.5.0">
      <module name="echarts" js="dist/echarts.js"/>
    </bundle>
    <bundle name="vue" version="2.6.11">
      <module name="vue" js="vue.js"/>
    </bundle>
  </static>
</mvc>
